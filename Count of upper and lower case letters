/* Counting the number of upper case and lower case letters in given string*/
#include<stdio.h>
int main()
{
	//Write code here
	char s[100000];
	int i,count1=0,count2=0;
	i=0;
	
	scanf("%[^\n]s",s);
	for(i=0;s[i]!='\0';i++)
	{
		if(s[i]>=60&&s[i]<=90)
		{
			count1=count1+1;
		}
		else if(s[i]>=97&&s[i]<=122)
		{
			count2=count2+1;
		}
		
	}

	printf("%d\n",count1);
	printf("%d",count2);
	return 0;

}
