/*Sum of elements in an array */
#include<stdio.h>
int main()
{
	int a[50],n,i,sum=0;
	printf("Enter size of array:");
	scanf("%d",&n);
	printf("Enter the array elements:");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	for(i=0;i<n;i++)
	{
		sum=sum+a[i];
		
	}
	printf("Sum of elements: %d",sum);
	return 0;
}
